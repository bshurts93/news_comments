<h1 class="text-center">{{message}}</h1>

<div class="container">
  <div class="article-box">
    {{#each articles}}
    <div class="article">
      <h3>{{title}}</h3>
      <p>{{content}}</p>
      <p>{{timestamp}}</p>
      <a href="{{link}}" target="_blank">Read more</a>
    </div>
    {{/each}}
  </div>
</div>



